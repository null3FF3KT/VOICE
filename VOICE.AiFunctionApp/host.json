{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxConcurrentRequests": 5,
      "maxOutstandingRequests": 10,
      "dynamicThrottlesEnabled": false
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "cors": {
    "allowedOrigins": [
      "http://localhost:8000",
      "https://lemon-moss-0c85a060f.5.azurestaticapps.net"
    ],
    "allowedMethods": [
      "GET",
      "POST",
      "OPTIONS"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Authorization",
      "X-Requested-With"
    ]
  }
}