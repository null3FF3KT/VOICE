{
  "version": "2.0",
  "extensions": {
    "http": {
      "routePrefix": "api",
      "maxConcurrentRequests": 5,
      "maxOutstandingRequests": 10,
      "dynamicThrottlesEnabled": false
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[3.*, 4.0.0)"
  },
  "cors": {
    "allowedOrigins": [
      "http://localhost:8000"
    ],
    "allowedMethods": [
      "GET",
      "POST",
      "OPTIONS"
    ]
  }
}