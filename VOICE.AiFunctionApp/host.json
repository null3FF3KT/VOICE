{
    "version": "2.0",
    "extensions": {
      "http": {
        "routePrefix": "api",
        "maxConcurrentRequests": 5,
        "maxOutstandingRequests": 10,
        "dynamicThrottlesEnabled": false
      }
    },
    "logging": {
      "applicationInsights": {
        "samplingExcludedTypes": "Request",
        "samplingSettings": {
          "isEnabled": true
        }
      }
    },
    "cors": {
      "allowedOrigins": [
        "http://localhost:8000"
      ],
      "allowedMethods": [
        "GET",
        "POST",
        "OPTIONS"
      ]
    }
  }
  